<script async src="https://www.youtube.com/iframe_api"></script>
<script src="javascripts/youtube.js"></script>
<div class="row">
<div class="jumbotron jumbotron-fluid">
  <div class='row'>
  <img class='col-md-6' src="/images/logo">
  <div class='col-md-6'>
  <h1 class="display-3"><%= @intro_message_header %></h1>
  <p class="lead"><%= @intro_message_lead %></p>
  </div>
  </div>
</div>

<div class="row">
<div class='col-md-6' id='assets'>
  <div id='youtube_video'></div>
</div>
<div class='col-md-6' id='announcements'>
  <h1>Announcements</h1>
  <ul>
    <% @announcements.each do |announcement| %>
    <li>
      <p><%= announcement %><p>
      </li>
      <% end %>
    </ul>
  </div>
</div>